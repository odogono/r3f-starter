{
  "name": "vite-react-starter",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix .",
    "outdated": "bunx npm-check-updates --interactive --format group",
    "preview": "vite preview",
    "unused": "knip",
    "coverage": "vitest run --coverage",
    "test": "vitest"
  },
  "dependencies": {
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^10.0.5",
    "@react-three/fiber": "^9.1.1",
    "jotai": "^2.12.2",
    "mitt": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@nkzw/eslint-config": "^2.2.0",
    "@react-three/eslint-plugin": "^0.1.2",
    "@tailwindcss/postcss": "^4.1.2",
    "@tailwindcss/vite": "^4.1.2",
    "@types/bun": "^1.2.8",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.23.0",
    "knip": "^5.46.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
  },
  "overrides": {
    "@react-spring/three": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    }
  }
}